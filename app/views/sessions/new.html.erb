
<h1>Welcome to Greenlink</h1>

<%= form_for @user, url: sessions_path, class: "form" do |f| %>
<%= image_tag "Bariloche_Argentina2.jpg" %>
	<%= f.text_field :username, placeholder: "username", class: "form-control"%>
	<%= password_field_tag :pasword, nil, class: "form-control", placeholder: "password" %>
	<%= f.submit "Log In", class: "btn btn-primary"%>
	<br>
<% end %>